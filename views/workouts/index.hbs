<div>
    <a href="/"><h1>Fitbrary</h1></a>
    <h2>Workouts Library</h2>
</div>
<div>
    <div>
        {{#each user}}
            {{#each workoutsCreated}}
            <a href="/users/{{../_id}}/workouts/{{this._id}}"><h3>{{this.workoutName}}</h3></a>
                
            {{/each}}
        {{/each}}
    </div>

    <div>
        <form action="/workouts/new">
            <input class="button" type="submit" value="New Workout" />
        </form>
        <form action="/users">
            <input class="button" type="submit" value="Users Library" />
        </form>
    </div>
    
</div>